{
    "name": "codebros-nl/laravel-mandrill-driver",
    "description": "Mandrill Driver for Laravel 10+",
    "type": "library",
    "keywords": [
        "Laravel",
        "Mandrill"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Norman Reinhard",
            "email": "norman@codebros.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "mailchimp/transactional": "^1.0",
        "symfony/mailer": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^v8.3.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMandrill\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMandrill\\MandrillServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
